  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BDBCDB66"
      }
    },
    {
      "name": "Comments Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "foreground": "#BDBCDB66"
      }
    },
    {
      "name": "Comments Documentation",
      "scope": [
        "comment.documentation",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#70C184"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#CC7F65"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#BDBCDB"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#BE54A7"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#BE54A7"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#A57EB3"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#BDBCDB"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#4F9DC4"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#4F9DC4"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#CC7F65"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#BE54A7"
      }
    },
    {
      "name": "Numbers Characters",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#A57EB3"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#4F9DC4"
      }
    },
    {
      "name": "Strings Escape Sequences",
      "scope": [
        "constant.character.escape",
        "constant.character"
      ],
      "settings": {
        "foreground": "#D6D65B"
      }
    },
    {
      "name": "Strings Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D6D65B"
      }
    },
    {
      "name": "Strings Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#BDBCDB"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#BDBCDB"
      }
    },
    {
      "name": "HTML Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#4F9DC4"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#BDBCDB"
      }
    },
    {
      "name": "HTML Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#BE54A7"
      }
    },
    {
      "name": "HTML Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A57EB3"
      }
    },
    {
      "name": "HTML Entities",
      "scope": [
